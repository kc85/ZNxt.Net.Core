@model ViewModelBase

    <div class="login-page">

        <div class="page-header">
            <h4>Forget Password</h4>
        </div>
        <partial name="_ValidationSummary" />
        <div class="row">
            <div class="col-sm-12">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <form>
                            <fieldset>
                                <div>
                                    <div>
                                        <div class="form-group">
                                            <label></label>
                                            <input type="email" class="form-control" placeholder="Enter your username" autocomplete="on">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <button class="btn btn-primary" name="button" id="btngetusername">Get password</button>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>